<xml xmlns="https://developers.google.com/blockly/xml"><block type="pxt-on-start" id="LmZQDt.zvG-l*V(6pdj;" x="0" y="0"><statement name="HANDLER"><block type="typescript_statement" id="0Y#iQdBtTwvGw.!XH=V:" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="namespace leds {" line1="    //% block: &quot;turns on and off the leds in an spiral form&quot;" line2="    export function spiral_led() {" line3="        for (let index = 0; index &lt; 2; index++) {" line4="            let i, l = 0, k = 0, n = 5, m = 5;" line5="            while (k &lt; m &amp;&amp; l &lt; n) {" line6="                for (i = l; i &lt; n; ++i) {" line7="                    led.toggle(k, i)" line8="                    pause(100)" line9="" line10="                }" line11="                k += 1" line12="                for (i = k; i &lt; m; ++i) {" line13="                    led.toggle(i, n - 1)" line14="                    pause(100)" line15="" line16="                }" line17="                n += -1" line18="                // print the last row from the remaining rows" line19="                if (k &lt; m) {" line20="                    for (i = n - 1; i &gt;= l; --i) {" line21="                        led.toggle(m - 1, i)" line22="                        pause(100)" line23="                    }" line24="                    m += -1" line25="                }" line26="                // print the first column from the remaining columns" line27="                if (l &lt; n) {" line28="                    for (i = m - 1; i &gt;= k; --i) {" line29="                        led.toggle(i, l)" line30="                        pause(100)" line31="" line32="                    }" line33="                    l += 1" line34="                }" line35="            }" line36="        }" line37="" line38="    }" line39="" line40="" line41="" line42="}" numlines="43"></mutation></block></statement></block></xml>